<nav>
<% content_for :visitor do %>
<% end %>

<% content_for :user do %>
<div class="user-nav">
  <% if !current_user.nil? %>
  <%= link_to "Home", root_path %>
  <%= link_to "Log Out", logout_path %>
  <%= link_to "Account", user_path(current_user) %>
  <% if !current_user.admin? %>
  <%= "Logged in as: #{current_user.username}" %>
  <% else %>
  <%= "Logged in as Admin User: #{current_user.username} "%>
  <% end %>
  <% end %>
</div>
<% end %>
<% content_for :admin do %>
<div class="admin-nav">
</div>
<% end %>
</nav>
