<ul>
  <li>Station Name: <h1><%= @station.name %></h1></li>
  <li>Dock Count: <%= @station.dock_count %></li>
  <li>City: <%= @station.city %></li>
  <li>Installation Date: <%= @station.installation_date.strftime("%m/%d/%Y") %></li>
  <% if !current_user.nil? %>
    <li>Number of Rides Started at this Station: <%= @station.started_at %></li>
    <li>Number of Rides Ended at this Station: <%= @station.ended_at %></li>
    <li>Popular Destination Station: <%= @station.popular_end_station %></li>
    <li>Popular Starting Station: <%= @station.popular_start_station %></li>
    <li>Popular Starting Date: <%= @station.popular_date.strftime("%m/%d/%Y") %></li>
    <li>Popular Zip Code of Users Starting at this Station: <%= @station.popular_zip %></li>
    <li>Bike ID most frequently starting a trip at this station: <%= @station.popular_bike %></li>
  <%end%>
  <% if !current_user.nil? && current_user.admin? %>
      <%= link_to 'Edit', edit_admin_station_path(@station) %>
      <%= link_to "Delete", admin_station_path(@station), method: "delete" %>
    <%end%>
</ul>
