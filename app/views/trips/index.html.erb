
<main>
  <h1>Trips</h1>
<table class="trips">
  <thead>
    <tr>
      <th>Duration</th>
      <th>Start Date</th>
      <th>Start Station</th>
      <th>End Date</th>
      <th>End Station</th>
      <th>Bike ID</th>
      <th>Subscription Type</th>
      <th>Zip Code</th>
    </tr>
    </thead>
      <tbody>
      <% @trips.each do |trip| %>
      <tr class=trip_<%= trip.id %>>
        <td><%= trip.duration %></td>
        <td><%= trip.start_date %></td>
        <td><%= trip.start_station %></td>
        <td><%= trip.end_date %></td>
        <td><%= trip.end_station %></td>
        <td><%= trip.bike_id %></td>
        <td><%= trip.subscription_type %></td>
        <td><%= trip.zip_code %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</main>

<%= will_paginate @trips %>
